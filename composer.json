{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"require": {
		"laravel/framework": "4.2.*",
        "jenssegers/mongodb": "*",
        "mj/breadcrumb": "dev-master",
        "anahkiasen/former": "dev-master",
        "intervention/image": "2.*",
        "intervention/imagecache": "dev-master",
        "intervention/helper": "dev-master",
        "intervention/validation": "dev-master",
        "maatwebsite/excel": "1.*",
        "alexwhitman/view-themes": "1.2.*",
        "barryvdh/laravel-dompdf": "dev-master",
        "knplabs/knp-snappy": "*",
        "barryvdh/laravel-snappy": "0.1.*",
        "flynsarmy/db-blade-compiler": "1.*",
        "pingpong/menus": "1.0.*",
        "bogardo/mailgun": "3.0.*"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php",
            "app/helpers",
            "app/library"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan optimize"
		],
		"pre-update-cmd": [

		],
		"post-update-cmd": [
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
